{
  "name": "@nordicsemiconductor/asset-tracker-cloud-device-ui-server",
  "version": "0.0.0-development",
  "description": "Provides the server for device-ui to interact with the simulated device",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/asset-tracker-cloud-device-ui-server-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/asset-tracker-cloud-device-ui-server-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/asset-tracker-cloud-device-ui-server-js",
  "keywords": [
    "Cloud",
    "IoT",
    "Asset Tracker Cloud Example",
    "Nordic Semiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "chalk": "4.1.0",
    "websocket": "1.0.33"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-angular": "11.0.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "9.0.2",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "7.0.5",
    "@types/websocket": "1.0.1",
    "husky": "5.1.1",
    "lint-staged": "10.5.4",
    "pinst": "2.1.4",
    "prettier": "2.2.1",
    "typescript": "4.1.5"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ],
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package-lock.json",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  }
}
