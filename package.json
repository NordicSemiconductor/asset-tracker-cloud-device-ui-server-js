{
  "name": "@bifravst/device-ui-server",
  "version": "0.0.0-development",
  "description": "Provides the server for device-ui to interact with the simulated device",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bifravst/device-ui-server.git"
  },
  "bugs": {
    "url": "https://github.com/bifravst/device-ui-server/issues"
  },
  "homepage": "https://github.com/bifravst/device-ui-server",
  "keywords": [
    "Cloud",
    "IoT",
    "bifravst",
    "AssetTracker",
    "NordicSemiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "websocket": "^1.0.30"
  },
  "devDependencies": {
    "@bifravst/code-style": "^4.1.1",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-angular": "^8.2.0",
    "@types/websocket": "1.0.0",
    "husky": "^3.0.7"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts",
      "git add"
    ],
    "*.{md,json,yaml,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "release": {
    "branch": "saga",
    "remoteTags": true
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "package-lock.json",
    "dist",
    "LICENSE",
    "README.md"
  ]
}
