{
  "name": "@nordicsemiconductor/asset-tracker-cloud-device-ui-server",
  "version": "0.0.0-development",
  "description": "Provides the server for device-ui to interact with the simulated device",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/asset-tracker-cloud-device-ui-server-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/asset-tracker-cloud-device-ui-server-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/asset-tracker-cloud-device-ui-server-js",
  "keywords": [
    "Cloud",
    "IoT",
    "nRF Asset Tracker",
    "Nordic Semiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "websocket": "1.0.34"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-angular": "12.1.4",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "9.0.4",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "8.0.1",
    "@types/websocket": "1.0.3",
    "husky": "7.0.1",
    "lint-staged": "11.0.1",
    "pinst": "2.1.6",
    "prettier": "2.3.2",
    "typescript": "4.3.5"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ],
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package-lock.json",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  }
}
