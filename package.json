{
  "name": "@bifravst/device-ui-server",
  "version": "0.0.0-development",
  "description": "Provides the server for device-ui to interact with the simulated device",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bifravst/device-ui-server.git"
  },
  "bugs": {
    "url": "https://github.com/bifravst/device-ui-server/issues"
  },
  "homepage": "https://github.com/bifravst/device-ui-server",
  "keywords": [
    "Cloud",
    "IoT",
    "bifravst",
    "Asset Tracker",
    "NordicSemiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "chalk": "^4.1.0",
    "websocket": "^1.0.32"
  },
  "devDependencies": {
    "@bifravst/code-style": "8.0.110",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-angular": "11.0.0",
    "@types/websocket": "1.0.1",
    "husky": "4.3.0",
    "typescript": "4.0.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ],
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package-lock.json",
    "dist",
    "LICENSE",
    "README.md"
  ]
}
